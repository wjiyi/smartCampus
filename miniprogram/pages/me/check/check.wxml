<!--pages/me/check/check.wxml-->
<view class='check-container'>
 <block wx:for="{{postList}}" wx:for-item="item" wx:for-index="idx" wx:key="postList">
  <view class='card' bindlongtap='goToInCheckDetails'>
      <text>{{item.name}}</text>
      <view class='button-style'>
        <button type='primary' class='pass'>通过</button>
        <button type='primary' class='pass'>不通过</button>
      </view>
    
  </view>
  </block>
</view>